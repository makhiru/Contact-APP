<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Contact" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#018EFF</item>
        <item name="colorPrimaryVariant">#018EFF</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="mytheme" parent="Theme.Design.Light.NoActionBar">
        <item name="colorPrimary">#018EFF</item>
        <item name="colorPrimaryDark">#018EFF</item>
        <item name="colorAccent">#018EFF</item>

    </style>

    <style name="addcontacts" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">#018EFF</item>
        <item name="colorPrimaryDark">#018EFF</item>
        <item name="colorAccent">#018EFF</item>

    </style>
</resources>